<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.org/dtd/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
<type-mapping>
	<sql-type jdbc-type="VARCHAR" hibernate-type="java.lang.String"></sql-type>
	<sql-type jdbc-type="NVARCHAR" hibernate-type='java.lang.String'
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="INTEGER" hibernate-type="int"
		not-null="true">
	</sql-type>
</type-mapping>
<table-filter match-name="Book" match-catalog="CCDataBase"
		match-schema="dbo">
	</table-filter>
	<table-filter match-name="BookDetail" match-catalog="CCDataBase"
		match-schema="dbo">
	</table-filter>
</hibernate-reverse-engineering>